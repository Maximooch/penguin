{
  "name": "@penguin/cli",
  "version": "0.1.0",
  "type": "module",
  "description": "Penguin AI - TypeScript CLI with Ink (React for Terminal)",
  "main": "dist/index.js",
  "bin": {
    "penguin-cli": "dist/index.js"
  },
  "scripts": {
    "dev": "npm run build && npm run fix-imports && node dist/index.js",
    "dev:mock": "npm run build && npm run fix-imports && node dist/prototype-mock.js",
    "dev:watch": "tsc --watch",
    "build": "tsc && npm run fix-imports",
    "fix-imports": "node scripts/fix-imports.js",
    "start": "node dist/index.js",
    "mock": "node dist/prototype-mock.js",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "penguin",
    "cli",
    "ink",
    "terminal",
    "ai",
    "assistant"
  ],
  "author": "Maximus Putnam",
  "license": "MIT",
  "dependencies": {
    "@inkjs/ui": "^2.0.0",
    "ansi-escapes": "^7.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "diff": "^7.0.0",
    "highlight.js": "^11.10.0",
    "ink": "^5.0.1",
    "ink-markdown": "^1.0.4",
    "ink-spinner": "^5.0.0",
    "react": "^18.3.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@swc/core": "^1.13.5",
    "@swc/helpers": "^0.5.17",
    "@types/diff": "^6.0.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "esbuild": "^0.25.11",
    "eslint": "^9.17.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
