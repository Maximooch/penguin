{
  "name": "@penguin/cli",
  "version": "0.1.0",
  "type": "module",
  "description": "Penguin AI - TypeScript CLI with Ink (React for Terminal)",
  "main": "dist/index.js",
  "bin": {
    "penguin-cli": "dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.tsx",
    "dev:watch": "tsx watch src/index.tsx",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "penguin",
    "cli",
    "ink",
    "terminal",
    "ai",
    "assistant"
  ],
  "author": "Maximus Putnam",
  "license": "MIT",
  "dependencies": {
    "ink": "^5.0.1",
    "@inkjs/ui": "^2.0.0",
    "react": "^18.3.1",
    "chalk": "^5.3.0",
    "ws": "^8.18.0",
    "diff": "^7.0.0",
    "commander": "^12.1.0",
    "highlight.js": "^11.10.0",
    "ansi-escapes": "^7.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/node": "^22.10.2",
    "@types/ws": "^8.5.13",
    "@types/diff": "^6.0.0",
    "typescript": "^5.7.2",
    "tsx": "^4.19.2",
    "eslint": "^9.17.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
